{
  "expo": {
    "name": "WireCart",
    "slug": "WireCartnative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"com.ecommerceapp.kaushik",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon2.png"
    }
  }
}
